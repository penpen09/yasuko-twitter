<%= render 'header' %>
<div class="container">
<h1>mytweets一覧</h1>
<p><%= notice %></p>
<table>
  <tr>
    <th>内容</th>
  </tr>
<% @mytweets.each do |mytweet| %>
  <tr>
    <td><%= mytweet.content %></td>
    <td><%= link_to '編集',edit_mytweet_path(mytweet.id),class:"btn btn-outline-info"%></td>
  </tr>
<% end %>
</table>
<%= link_to 'つぶやく', new_mytweet_path, class:"btn btn-primary"%>
</div>
<%= render 'footer' %>
