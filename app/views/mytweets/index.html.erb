<%= render 'header' %>
<div class="container">
<div class="content">
<h1 class="text-left">mytweets一覧</h1>
<p><%= notice %></p>
<table>
  <tr>
    <th>内容</th>
  </tr>
<% @mytweets.each do |mytweet| %>
  <tr>
    <td><%= mytweet.content %></td>
    <td><%= link_to '編集',edit_mytweet_path(mytweet.id),class:"btn btn-outline-info"%></td>
    <td><%= link_to '削除',mytweet_path(mytweet.id),method: :delete, data:{ confirm:'本当に削除しますか？'}, class:"btn btn-outline-danger"%></td>
  </tr>
<% end %>
</table>
<%= link_to 'つぶやく', new_mytweet_path, class:"btn btn-warning"%>
</div>
</div>
<%= render 'footer' %>
